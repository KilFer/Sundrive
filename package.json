{
  "name": "sundrive",
  "author": "FBelaza",
  "version": "0.1.0",
  "keywords": [
    "pebble-app"
  ],
  "private": true,
  "dependencies": {
    "@rebble/clay": "^1.0.6"
  },
  "pebble": {
